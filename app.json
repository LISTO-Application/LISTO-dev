{
  "expo": {
    "name": "LISTO-dev",
    "slug": "LISTO-dev",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/listo-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#115272"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pingusbingus.LISTOdev",
      "googleServicesFile": "./google-services.json"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "bundleIdentifier": "com.pingusbingus.LISTOdev",
        "backgroundColor": "#115272"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBa31nHNFvIEsYo2D9NXjKmMYxT0lwE6W0"
        }
      },
      "permissions": ["android.permission.WRITE_EXTERNAL_STORAGE"],
      "package": "com.pingusbingus.LISTOdev",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c90d6a18-1dbc-460c-9ea4-6e9e125a22b6"
      }
    },
    "owner": "listo-dev"
  }
}
